package de.monticore.lang.templatesignature;

/**
 * Grammar for Template signatures.
 *
 * @author Jerome Pfeiffer
 */
grammar TemplateSignature extends de.monticore.literals.Literals, de.monticore.types.Types {

  Comment = "<#--" Signature "-->";
  
  Signature = comment:Name*  parameters:Parameter* result:Result?;
  
  Parameter = "@param" paramType:QualifiedName name:Name;
  
  Result = "@result" resultType:QualifiedName;
    
}
