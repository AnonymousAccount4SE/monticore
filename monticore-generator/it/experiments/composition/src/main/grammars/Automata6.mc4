grammar Automata6 extends de.monticore.MCBasics {

  symbol Automaton = "automaton" Name "{" (State | Transition)* "}" ;

  symbol State = "state" Name ";" ;

  Transition = from:Name "-" input:Name@Stimulus ">" to:Name ";" ;

  symbol Stimulus = Name;
  
}
