package de.monticore.types;

/**
 * This grammar introduces basic generics List<> and Optional<>
*/
component grammar MCBasicGenericsTypes extends de.monticore.types.MCBasicTypes {


  interface ReferenceType;

  ListType implements ReferenceType, Type = "List<" SimpleReferenceType ">";

  OptionalType implements ReferenceType, Type = "Optional<" SimpleReferenceType ">";

}