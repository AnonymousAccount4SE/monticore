package de.monticore.types;

/**
 * This grammar introduces basic generics List<> and Optional<>
*/
component grammar MCBasicGenericsTypes extends de.monticore.types.MCBasicTypes {

  start Type;

  ListType implements ReferenceType = "List<" Type ">";

  OptionalType implements ReferenceType = "Optional<" Type ">";

  MapType implements ReferenceType = "Map<" Type "," Type ">";

  SetType implements ReferenceType = "Set<" Type ">";
}