package de.monticore.types;

component grammar TypeSymbols extends de.monticore.MCBasics {

  symbol Type = Name (Field|Method)*;

  symbol Field = Name ;


  symbolrule  Field =
    type: de.monticore.types.typesymbols._symboltable.TypeSymbol
;

  symbol Method = Name;
  symbolrule Method =
    parameters: de.monticore.types.typessymbols._symboltable.FieldSymbol
  ;
}